{
  "name": "angular-cli-meteor",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "api": "cd api && meteor run",
    "client-bundle": "meteor-client bundle -s api",
    "start:dev": "run-p start:api start:client",
    "start:api": "wait-on -d 500 node_modules/meteor-client.js && npm run api",
    "prestart:api": "path-exists node_modules/meteor-client.js || npm run client-bundle",
    "start:client": "wait-on node_modules/meteor-client.js && npm run start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.7",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@babel/runtime": "7.0.0-beta.55",
    "core-js": "^2.5.7",
    "meteor-client-bundler": "^0.5.1",
    "meteor-node-stubs": "^0.4.1",
    "meteor-rxjs": "^0.4.14",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.4",
    "@angular/cli": "~6.1.4",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/meteor": "1.4.16",
    "@types/node": "~10.7.1",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.3",
    "path-exists": "^3.0.0",
    "path-exists-cli": "^1.0.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2",
    "wait-on": "^2.1.0"
  }
}
